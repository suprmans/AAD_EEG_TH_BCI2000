        <mrow class="MJX-TeXAtom-ORD">
          <mrow class="MJX-TeXAtom-ORD">
            <mtext mathvariant="monospace">mPreviousOutput[]</mtext>
          </mrow>
        </mrow>
        <mo>=</mo>
