          <mrow class="MJX-TeXAtom-ORD">
            <mtext mathvariant="monospace">mDecayFactor</mtext>
          </mrow>
        </mrow>
        <mo>=</mo>
        <msup>
          <mi>e</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mo>&#x2212;<!-- − --></mo>
            <mn>1</mn>
            <mrow class="MJX-TeXAtom-ORD">
